{{
    config(
         materialized='table'
    )
}} 

WITH exchange_cte AS (

    SELECT Distinct SECURITYEXCHANGE FROM CME_DATABASE.PUBLIC.METALS 
    UNION 
    SELECT Distinct SECURITYEXCHANGE FROM CME_DATABASE.PUBLIC.COMMODITY_AGR
    UNION 
    SELECT Distinct SECURITYEXCHANGE FROM CME_DATABASE.PUBLIC.EQUITY
    UNION 
    SELECT Distinct SECURITYEXCHANGE FROM CME_DATABASE.PUBLIC.INTEREST_RATE 
    UNION 
    SELECT Distinct SECURITYEXCHANGE FROM CME_DATABASE.PUBLIC.ENERGY 
    UNION 
    SELECT Distinct SECURITYEXCHANGE FROM CME_DATABASE.PUBLIC.CURRENCY
)

SELECT
SECURITYEXCHANGE

FROM
exchange_cte
