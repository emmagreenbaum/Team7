{{
    config(
         materialized='table'
    )
}} WITH security_cte AS (

    SELECT Distinct SECURITYTYPE, FROM CME_DATABASE.PUBLIC.METALS 
    UNION 
    SELECT Distinct SECURITYTYPE FROM CME_DATABASE.PUBLIC.COMMODITY_AGR
    UNION 
    SELECT Distinct SECURITYTYPE FROM CME_DATABASE.PUBLIC.EQUITY
    UNION 
    SELECT Distinct SECURITYTYPE FROM CME_DATABASE.PUBLIC.INTEREST_RATE 
    UNION 
    SELECT Distinct SECURITYTYPE FROM CME_DATABASE.PUBLIC.ENERGY 
    UNION 
    SELECT Distinct SECURITYTYPE FROM CME_DATABASE.PUBLIC.CURRENCY
)

SELECT
SECURITYTYPE

FROM
security_cte
